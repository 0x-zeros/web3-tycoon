{
  /* Base configuration. Do not edit this field. */
  "extends": "./temp/tsconfig.cocos.json",

  /* Add your custom configuration here. */
  "compilerOptions": {
    "strict": false,

    "target": "ES2020",
    "module": "ES2020",
    "lib": ["ES2020", "DOM"],
    "skipLibCheck": true,

    /* 增强模块兼容性 */
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  }
}
