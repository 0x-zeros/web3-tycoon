  },
  "keywords": [
    "cloudflare",
    "workers",
    "web3",
    "sui",
    "tycoon"
  ],
  "author": "Web3 Tycoon Team",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "wrangler": "^3.22.1",
    "typescript": "^5.3.3"
  }
}
