{
  "name": "web3-tycoon-api",
  "version": "1.0.0",
  "description": "Cloudflare Workers API for Web3 Tycoon metadata service",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
